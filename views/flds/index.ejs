<h2 class="page-header">FieLDs <a href="/flds/new">Add</a></h2>
<form>
  <div>
    <label>field</label>
    <input type="text" name="field" value="<%= searchOptions.field %>" />
  </div>
  <button type="submit">Search</button>
</form>
<% flds.forEach(fld => { %>
<div><%= fld.field %></div>
<a href="/flds/<%= fld.id %>">View</a>
<a href="/flds/<%= fld.id %>/edit">Edit</a>
<%- include('../partials/deleteForm', { url: `/flds/${fld.id}`}) %> <% }) %>
